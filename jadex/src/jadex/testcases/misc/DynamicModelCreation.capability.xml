<capability xmlns="http://jadex.sourceforge.net/jadex"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jadex.sourceforge.net/jadex 
	                    http://jadex.sourceforge.net/jadex-0.96.xsd"
	package="jadex.testcases.misc"
	name="DynamicModelCreation">
	
	<goals>
		<achievegoal name="capagoal" exported="true">
 			<parameter name="param" class="String">
				 <value>"default"</value>
 			</parameter>
		</achievegoal>
	</goals>
	
	<plans>
		<plan name="capaplan">
			<parameter name="param" class="String">
				<internaleventmapping ref="capaevent.param"/>
				<goalmapping ref="capagoal.param"/>
			</parameter>
			<body>new jadex.testcases.ResultPlan($plan.param)</body>
			<trigger>
				<internalevent ref="capaevent"/>
				<goal ref="capagoal"/>
			</trigger>
		</plan>
	</plans>

	<events>
		<internalevent name="capaevent">
			<parameter name="param" class="String"/>
		</internalevent>
	</events>

</capability>