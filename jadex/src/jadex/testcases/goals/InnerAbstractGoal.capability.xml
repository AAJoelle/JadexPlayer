<!--
	<H3>Test abstract goal dispatched by inner capability.</H3>

	Defines an abstract goal and a test plan dispatching it.
-->
<capability xmlns="http://jadex.sourceforge.net/jadex"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jadex.sourceforge.net/jadex 
	                    http://jadex.sourceforge.net/jadex-0.96.xsd"
	name="InnerAbstractGoal"
	package="jadex.testcases.goals">
	
	<beliefs>
		<beliefsetref name="reports">
			<abstract/>
		</beliefsetref>
	</beliefs>

	<goals>
		<achievegoalref name="testgoal" exported="true">
			<abstract/>
			<parameterref name="param" class="String">
				<abstract/>
			</parameterref>
			<parameterref name="result" class="String">
				<abstract/>
			</parameterref>
		</achievegoalref>
	</goals>
	
	<plans>
		<plan name="generate_goal_plan">
			<body class="GenerateGoalPlan"/>
			<!-- <body>new GenerateGoalPlan()</body> -->
		</plan>
	</plans>

	<configurations>
		<configuration name="default">
			<plans>
				<initialplan ref="generate_goal_plan"/>
			</plans>
		</configuration>
	</configurations>

</capability>

