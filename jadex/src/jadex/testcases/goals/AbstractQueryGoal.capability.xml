<!--
	<H3>Test abstract query goal dispatched by inner capability.</H3>

	Dispatches an abstract query goal.
-->
<capability xmlns="http://jadex.sourceforge.net/jadex"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jadex.sourceforge.net/jadex 
	                    http://jadex.sourceforge.net/jadex-0.96.xsd"
	name="AbstractQueryGoal"
	package="jadex.testcases.goals">
	
	<beliefs>
		<beliefsetref name="reports">
			<abstract/>
		</beliefsetref>
	</beliefs>

	<goals>
		<querygoalref name="testgoal" exported="true">
			<abstract/>
			<parametersetref name="params" class="String">
				<abstract/>
			</parametersetref>
			<parametersetref name="results" class="String">
				<abstract/>
			</parametersetref>
		</querygoalref>
	</goals>
	
	<plans>
		<plan name="generate_goal_plan">
			<body class="GenerateGoalPlan"/>
			<!-- <body>new GenerateGoalPlan()</body> -->
		</plan>
	</plans>

	<configurations>
		<configuration name="default">
			<plans>
				<initialplan ref="generate_goal_plan"/>
			</plans>
		</configuration>
	</configurations>

</capability>

