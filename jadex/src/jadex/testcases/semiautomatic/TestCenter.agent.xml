<!--
	<H3>Perform tests.</H3>

	The test center agent has the purpose to
	perform tests and collect the results.
-->

<agent xmlns="http://jadex.sourceforge.net/jadex"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jadex.sourceforge.net/jadex
	                    http://jadex.sourceforge.net/jadex-0.96.xsd"
	name="TestCenter"
	package="jadex.testcases.semiautomatic">

	<imports>
		<import>jadex.adapter.fipa.*</import>
		<import>jadex.planlib.*</import>
	</imports>

	<capabilities>
		<capability name="testcentercap" file="jadex.planlib.TestCenter"/>
	</capabilities>

	<goals>
		<performgoalref name="perform_tests">
			<concrete ref="testcentercap.perform_tests"/>
		</performgoalref>
	</goals>

	<properties>
        <!--<property name="debugging">true</property>-->
        <property name="logging.level">java.util.logging.Level.INFO</property>
    </properties>

	<configurations>
		<configuration name="default">
			<goals>
				<initialgoal ref="perform_tests">
					<parameterset ref="testcases">
						<value>new Testcase("jadex.testcases.misc.NoPackage")</value>
						<value>new Testcase("jadex.testcases.plans.BeliefTrigger")</value>
						<value>new Testcase("jadex.testcases.events.ProtocolInitiator")</value>
						<value>new Testcase("jadex.testcases.plans.WaitForBeliefSet")</value>
						<value>new Testcase("jadex.testcases.plans.PlanTermination")</value>
						<value>new Testcase("jadex.testcases.misc.Serialization")</value>
						<value>new Testcase("jadex.testcases.misc.GUICloser")</value>
						<value>new Testcase("jadex.testcases.misc.WaitFor")</value>
						<value>new Testcase("jadex.testcases.misc.DFTest")</value>
						<value>new Testcase("jadex.testcases.beliefs.BeliefSetChanges")</value>
						<value>new Testcase("jadex.testcases.plans.PlanPriorities")</value>
						<value>new Testcase("jadex.testcases.plans.PlanCondition")</value>
						<value>new Testcase("jadex.testcases.events.SendAndWait")</value>
						<value>new Testcase("jadex.testcases.goals.OuterAbstractGoal")</value>
						<value>new Testcase("jadex.testcases.capabilities.DynamicCapability")</value>
						<value>new Testcase("jadex.testcases.goals.MaintainGoalContext")</value>
						<value>new Testcase("jadex.testcases.goals.QueryGoal")</value>
						<value>new Testcase("jadex.testcases.goals.GoalConditions")</value>
						<value>new Testcase("jadex.testcases.events.MessageMatching")</value>
						<value>new Testcase("jadex.testcases.plans.Precondition")</value>
						<value>new Testcase("jadex.testcases.plans.WaitForBelief")</value>
						<value>new Testcase("jadex.testcases.goals.GoalBindings")</value>
						<value>new Testcase("jadex.testcases.goals.InnerAbstractGoal")</value>
						<value>new Testcase("jadex.testcases.misc.DFMobile")</value>
						<value>new Testcase("jadex.testcases.misc.AMSTest")</value>
						<value>new Testcase("jadex.testcases.plans.PlanAbort")</value>
						<value>new Testcase("jadex.testcases.misc.ParameterProtection")</value>
