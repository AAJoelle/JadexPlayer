<!--
	<H3>Test passing the arguments from starter dialog.</H3>
	When started will print all arguments to the sys.out.
-->

<agent xmlns="http://jadex.sourceforge.net/jadex"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xsi:schemaLocation="http://jadex.sourceforge.net/jadex
	                    http://jadex.sourceforge.net/jadex-0.96.xsd"
	   name="Arguments"
	   package="jadex.testcases.semiautomatic">

	<plans>
		<plan name="print">
			<body>new ArgumentsPlan($args)</body>
		</plan>
	</plans>

	<properties>
		<property name="logging.level">java.util.logging.Level.FINE</property>
	</properties>

	<configurations>
		<configuration name="default">
			<plans>
				<initialplan ref="print"/>
			</plans>
		</configuration>
	</configurations>

</agent>

