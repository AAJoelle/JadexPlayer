<!--
	<H3>Test array class as expression value.</H3>

	Tests if a special array expression can be loaded.
	<p>
	<b>This testcase fails with janino parser due to a bug in janino.
	Works with javacc parser.</b>
-->
<agent xmlns="http://jadex.sourceforge.net/jadex"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jadex.sourceforge.net/jadex
	                    http://jadex.sourceforge.net/jadex-0.96.xsd"
	name="Array"
	package="jadex.testcases.beliefs">

	<imports>
		<import>jadex.planlib.*</import>
	</imports>

	<capabilities>
		<capability name="testcap" file="jadex.planlib.Test"/>
	</capabilities>

	<beliefs>
		<beliefsetref name="reports">
			<concrete ref="testcap.reports"/>
		</beliefsetref>
		<beliefref name="timeout" class="long" exported="true">
			<concrete ref="testcap.timeout"/>
		</beliefref>
		<beliefref name="testcenter" class="jadex.adapter.fipa.AgentIdentifier" exported="true">
			<concrete ref="testcap.testcenter"/>
		</beliefref>

		<belief name="a" class="Class">
			<fact>String[].class</fact>
		</belief>
	</beliefs>

	<configurations>
		<configuration name="default">
			<beliefs>
				<initialbeliefset ref="reports">
					<fact>new TestReport("#1", "Test if agent can be loaded.", true, null)</fact>
				</initialbeliefset>
			</beliefs>
		</configuration>
	</configurations>

</agent>