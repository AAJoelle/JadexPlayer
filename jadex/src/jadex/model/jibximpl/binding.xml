<?xml version="1.0" encoding="UTF-8"?>
<binding track-source="true">
	<namespace uri="http://jadex.sourceforge.net/jadex" default="elements"/>
	
	<mapping name="agent" class="jadex.model.jibximpl.Agent" pre-set="preset">
		<structure map-as="jadex.model.jibximpl.MBDIAgent"></structure>
	</mapping>

	<mapping name="capability" class="jadex.model.jibximpl.Capability" pre-set="preset">
		<structure map-as="jadex.model.jibximpl.MCapability"></structure>
	</mapping>

	<mapping name="properties" class="jadex.model.jibximpl.Properties" pre-set="preset">
		<structure map-as="jadex.model.jibximpl.MPropertybase"></structure>
	</mapping>

	<mapping abstract="true" class="jadex.model.jibximpl.MBDIAgent" pre-set="preset">
		<structure map-as="jadex.model.jibximpl.MCapability"></structure>
		<value name="propertyfile" get-method="getPropertyFile" set-method="setPropertyFile" usage="optional" default="jadex.config.runtime" style="attribute"/>
	</mapping>

	<mapping abstract="true" class="jadex.model.jibximpl.MCapability" pre-set="preset">
		<structure map-as="jadex.model.jibximpl.MElement"></structure>
		<structure name="imports" usage="optional">
			<collection add-method="createImport" iter-method="iterImports" item-type="java.lang.String">
				<value name="import"/>
			</collection>
		</structure>
		<structure name="capabilities" usage="optional">
			<collection add-method="addCapabilityReference" iter-method="iterCapabilityReferences" item-type="jadex.model.jibximpl.MCapabilityReference">
				<structure name="capability" type="jadex.model.jibximpl.MCapabilityReference" pre-set="preset"></structure>
			</collection>
		</structure>
		<structure name="beliefs" field="beliefbase" usage="optional" type="jadex.model.jibximpl.MBeliefbase" pre-set="preset"></structure>
		<structure name="goals" field="goalbase" usage="optional" type="jadex.model.jibximpl.MGoalbase" pre-set="preset"></structure>
		<structure name="plans" field="planbase" usage="optional" type="jadex.model.jibximpl.MPlanbase" pre-set="preset"></structure>
		<structure name="events" field="eventbase" usage="optional" type="jadex.model.jibximpl.MEventbase" pre-set="preset"></structure>
		<structure name="expressions" field="expressionbase" usage="optional" type="jadex.model.jibximpl.MExpressionbase" pre-set="preset"></structure>
		<structure name="properties" field="propertybase" usage="optional" type="jadex.model.jibximpl.MPropertybase" pre-set="preset"></structure>
		<structure name="configurations" field="configurationbase" usage="optional" type="jadex.model.jibximpl.MConfigurationbase" pre-set="preset"></structure>
		<value name="package" field="packagename" usage="optional" style="attribute"/>
		<value name="abstract" field="isabstract" usage="optional" default="false" style="attribute"/>
	</mapping>

	<mapping class="jadex.model.jibximpl.MBeliefbase" abstract="true">
		<structure map-as="jadex.model.jibximpl.MBase"></structure>
		<collection add-method="addElement" iter-method="iterElements" ordered="false" item-type="jadex.model.jibximpl.MReferenceableElement">
			<structure name="belief" usage="optional" type="jadex.model.jibximpl.MBelief" pre-set="preset"></structure>
			<structure name="beliefset" usage="optional" type="jadex.model.jibximpl.MBeliefSet" pre-set="preset"></structure>
			<structure name="beliefref" usage="optional" type="jadex.model.jibximpl.MBeliefReference" pre-set="preset"></structure>
			<structure name="beliefsetref" usage="optional" type="jadex.model.jibximpl.MBeliefSetReference" pre-set="preset"></structure>
		</collection>
	</mapping>

	<mapping class="jadex.model.jibximpl.MGoalbase" abstract="true">
		<structure map-as="jadex.model.jibximpl.MBase"></structure>
		<collection add-method="addElement" iter-method="iterElements" ordered="false" item-type="jadex.model.jibximpl.MReferenceableElement">
			<structure name="performgoal" usage="optional" type="jadex.model.jibximpl.MPerformGoal" pre-set="preset"></structure>
			<structure name="achievegoal" usage="optional" type="jadex.model.jibximpl.MAchieveGoal" pre-set="preset"></structure>
			<structure name="querygoal" usage="optional" type="jadex.model.jibximpl.MQueryGoal" pre-set="preset"></structure>
			<structure name="maintaingoal" usage="optional" type="jadex.model.jibximpl.MMaintainGoal" pre-set="preset"></structure>
			<structure name="metagoal" usage="optional" type="jadex.model.jibximpl.MMetaGoal" pre-set="preset"></structure>
			<structure name="performgoalref" usage="optional" type="jadex.model.jibximpl.MPerformGoalReference" pre-set="preset"></structure>
			<structure name="achievegoalref" usage="optional" type="jadex.model.jibximpl.MAchieveGoalReference" pre-set="preset"></structure>
			<structure name="querygoalref" usage="optional" type="jadex.model.jibximpl.MQueryGoalReference" pre-set="preset"></structure>
			<structure name="maintaingoalref" usage="optional" type="jadex.model.jibximpl.MMaintainGoalReference" pre-set="preset"></structure>
			<structure name="metagoalref" usage="optional" type="jadex.model.jibximpl.MMetaGoalReference" pre-set="preset"></structure>
		</collection>
	</mapping>

	<mapping class="jadex.model.jibximpl.MPlanbase" abstract="true">
		<structure map-as="jadex.model.jibximpl.MBase"></structure>
		<collection add-method="addPlan" iter-method="iterPlans" item-type="jadex.model.jibximpl.MPlan">
			<structure name="plan" type="jadex.model.jibximpl.MPlan" pre-set="preset"></structure>
		</collection>
	</mapping>

	<mapping class="jadex.model.jibximpl.MEventbase" abstract="true">
		<structure map-as="jadex.model.jibximpl.MBase"></structure>
		<collection add-method="addElement" iter-method="iterElements" ordered="false" item-type="jadex.model.jibximpl.MReferenceableElement">
			<structure name="internalevent" usage="optional" type="jadex.model.jibximpl.MInternalEvent" pre-set="preset"></structure>
			<structure name="messageevent" usage="optional" type="jadex.model.jibximpl.MMessageEvent" pre-set="preset"></structure>
			<structure name="internaleventref" usage="optional" type="jadex.model.jibximpl.MInternalEventReference" pre-set="preset"></structure>
			<structure name="messageeventref" usage="optional" type="jadex.model.jibximpl.MMessageEventReference" pre-set="preset"></structure>
		</collection>
	</mapping>

	<mapping class="jadex.model.jibximpl.MExpressionbase" abstract="true">
		<structure map-as="jadex.model.jibximpl.MBase"></structure>
		<collection add-method="addElement" iter-method="iterElements" ordered="false" item-type="jadex.model.jibximpl.MReferenceableElement">
			<structure name="expression" usage="optional" type="jadex.model.jibximpl.MExpression" pre-set="preset"></structure>
			<structure name="condition" usage="optional" type="jadex.model.jibximpl.MCondition" pre-set="preset"></structure>
			<structure name="expressionref" usage="optional" type="jadex.model.jibximpl.MExpressionReference" pre-set="preset"></structure>
			<structure name="conditionref" usage="optional" type="jadex.model.jibximpl.MConditionReference" pre-set="preset"></structure>
		</collection>
	</mapping>

	<mapping class="jadex.model.jibximpl.MPropertybase" abstract="true">
		<structure map-as="jadex.model.jibximpl.MBase"></structure>
		<collection add-method="addProperty" iter-method="iterProperties" item-type="jadex.model.jibximpl.MExpression">
			<structure name="property" type="jadex.model.jibximpl.MExpression" pre-set="preset"></structure>
		</collection>
	</mapping>

	<mapping abstract="true" class="jadex.model.jibximpl.MGoal">
		<structure map-as="jadex.model.jibximpl.MParameterElement"></structure>
		<structure name="unique" field="unique" usage="optional" type="jadex.model.jibximpl.MUnique" pre-set="preset"></structure>
		<structure name="creationcondition" field="creationcondition" usage="optional" type="jadex.model.jibximpl.MBindingCondition" pre-set="preset"></structure>
		<structure name="contextcondition" field="contextcondition" usage="optional" type="jadex.model.jibximpl.MCondition" pre-set="preset"></structure>
		<structure name="dropcondition" field="dropcondition" usage="optional" type="jadex.model.jibximpl.MCondition" pre-set="preset"></structure>
		<structure name="deliberation" field="deliberation" usage="optional" type="jadex.model.jibximpl.MDeliberation" pre-set="preset"></structure>
		<value name="retry" field="retry" usage="optional" default="true" style="attribute"/>
		<value name="posttoall" field="posttoall" usage="optional" default="false" style="attribute"/>
		<value name="randomselection" field="randomselection" usage="optional" default="false" style="attribute"/>
		<value name="retrydelay" field="retrydelay" usage="optional" default="0" style="attribute"/>
		<value name="exclude" field="excludemode" usage="optional" default="when_tried" style="attribute"/>
		<value name="recalculate" field="recalculate" usage="optional" default="true" style="attribute"/>
		<value name="recur" field="recur" usage="optional" default="false" style="attribute"/>
		<value name="recurdelay" field="recurdelay" usage="optional" default="0" style="attribute"/>
	</mapping>

	<mapping class="jadex.model.jibximpl.MPerformGoal" abstract="true">
		<structure map-as="jadex.model.jibximpl.MGoal"></structure>
	</mapping>

	<mapping class="jadex.model.jibximpl.MAchieveGoal" abstract="true">
		<structure map-as="jadex.model.jibximpl.MGoal"></structure>
		<structure name="targetcondition" field="targetcondition" usage="optional" type="jadex.model.jibximpl.MCondition" pre-set="preset"></structure>
		<structure name="failurecondition" field="failurecondition" usage="optional" type="jadex.model.jibximpl.MCondition" pre-set="preset"></structure>
	</mapping>

	<mapping class="jadex.model.jibximpl.MQueryGoal" abstract="true">
		<structure map-as="jadex.model.jibximpl.MGoal"></structure>
		<!-- todo: remove target and failure conditions . -->
		<structure name="targetcondition" field="targetcondition" usage="optional" type="jadex.model.jibximpl.MCondition" pre-set="preset"></structure>
		<structure name="failurecondition" field="failurecondition" usage="optional" type="jadex.model.jibximpl.MCondition" pre-set="preset"></structure>
	</mapping>

	<mapping class="jadex.model.jibximpl.MMaintainGoal" abstract="true">
		<structure map-as="jadex.model.jibximpl.MGoal"></structure>
		<structure name="maintaincondition" field="maintaincondition" usage="required" type="jadex.model.jibximpl.MCondition" pre-set="preset"></structure>
		<structure name="targetcondition" field="targetcondition" usage="optional" type="jadex.model.jibximpl.MCondition" pre-set="preset"></structure>
	</mapping>

	<mapping class="jadex.model.jibximpl.MMetaGoal" abstract="true">
		<structure map-as="jadex.model.jibximpl.MQueryGoal"></structure>
		<structure name="trigger" field="trigger" usage="required" type="jadex.model.jibximpl.MMetaGoalTrigger" pre-set="preset"></structure>
	</mapping>

	<mapping class="jadex.model.jibximpl.MMetaGoalTrigger" abstract="true">
		<structure map-as="jadex.model.jibximpl.MTrigger"></structure>
		<collection add-method="addGoal" iter-method="iterGoals" item-type="jadex.model.jibximpl.MReference">
			<structure name="goal" type="jadex.model.jibximpl.MReference" pre-set="preset"></structure>
		</collection>
	</mapping>

	<mapping class="jadex.model.jibximpl.MPlan" abstract="true">
		<!--<structure map-as="jadex.model.jibximpl.MParameterElement"></structure>-->
		<structure map-as="jadex.model.jibximpl.MReferenceableElement"></structure>
		<collection add-method="addParameter" iter-method="iterParameters" item-type="jadex.model.IMParameter">
			<structure name="parameter" type="jadex.model.jibximpl.MPlanParameter" pre-set="preset"></structure>
		</collection>
		<collection add-method="addParameterSet" iter-method="iterParameterSets" item-type="jadex.model.IMParameterSet">
			<structure name="parameterset" type="jadex.model.jibximpl.MPlanParameterSet" pre-set="preset"></structure>
		</collection>
		<!-- exchange parameters by plan parameters. -->
		<structure name="body" field="body" usage="required" type="jadex.model.jibximpl.MPlanBody" pre-set="preset"></structure>
		<structure name="trigger" field="trigger" usage="optional" type="jadex.model.jibximpl.MPlanTrigger" pre-set="preset"></structure>
		<structure name="precondition" field="precondition" usage="optional" type="jadex.model.jibximpl.MExpression" pre-set="preset"></structure>
		<structure name="contextcondition" field="contextcondition" usage="optional" type="jadex.model.jibximpl.MCondition" pre-set="preset"></structure>
		<structure name="waitqueue" field="waitqueue" usage="optional" type="jadex.model.jibximpl.MTrigger" pre-set="preset"></structure>
		<value name="priority" field="priority" usage="optional" default="0" style="attribute"/>
	</mapping>

	<mapping class="jadex.model.jibximpl.MPlanBody" abstract="true">
		<structure map-as="jadex.model.jibximpl.MExpression"></structure>
		<value name="type" field="type" usage="optional" default="standard" style="attribute"/>
		<value name="inline" field="inline" usage="optional" default="false" style="attribute"/>
		<structure name="passed" usage="optional">
 			<value style="text" usage="required" field="passedcode"/>
		</structure>
		<structure name="failed" usage="optional">
 			<value style="text" usage="required" field="failedcode"/>
		</structure>
		<structure name="aborted" usage="optional">
 			<value style="text" usage="required" field="abortedcode"/>
		</structure>
	</mapping>

	<mapping class="jadex.model.jibximpl.MPlanTrigger" abstract="true">
		<structure map-as="jadex.model.jibximpl.MTrigger"></structure>
		<collection add-method="addGoal" iter-method="iterGoals" item-type="jadex.model.jibximpl.MReference">
			<structure name="goal" type="jadex.model.jibximpl.MReference" pre-set="preset"></structure>
		</collection>
		<structure name="condition" field="condition" usage="optional" type="jadex.model.jibximpl.MBindingCondition" pre-set="preset"></structure>
		<collection add-method="createBeliefChange" iter-method="iterBeliefChanges" item-type="java.lang.String">
			<structure name="beliefchange">
				<value name="ref" style="attribute"/>
			</structure>
		</collection>
		<collection add-method="createBeliefSetChange" iter-method="iterBeliefSetChanges" item-type="java.lang.String">
			<structure name="beliefsetchange">
				<value name="ref" style="attribute"/>
			</structure>
		</collection>
		<collection add-method="createFactAddedTrigger" iter-method="iterFactAddedTriggers" item-type="java.lang.String">
			<structure name="factadded">
				<value name="ref" style="attribute"/>
			</structure>
		</collection>
		<collection add-method="createFactRemovedTrigger" iter-method="iterFactRemovedTriggers" item-type="java.lang.String">
			<structure name="factremoved">
				<value name="ref" style="attribute"/>
			</structure>
		</collection>
	</mapping>

	<mapping abstract="true" class="jadex.model.jibximpl.MTypedElement">
		<structure map-as="jadex.model.jibximpl.MReferenceableElement"></structure>
		<value name="class" field="classname" usage="required" style="attribute"/>
		<value name="updaterate" field="updaterate" usage="optional" default="0" style="attribute"/>
		<value name="transient" field="trans" usage="optional" default="false" style="attribute"/>
	</mapping>

	<mapping abstract="true" class="jadex.model.jibximpl.MTypedElementSet">
		<structure map-as="jadex.model.jibximpl.MReferenceableElement"></structure>
		<value name="class" field="classname" usage="required" style="attribute"/>
		<value name="updaterate" field="updaterate" usage="optional" default="0" style="attribute"/>
		<value name="transient" field="trans" usage="optional" default="false" style="attribute"/>
	</mapping>

	<mapping class="jadex.model.jibximpl.MBelief" abstract="true">
		<structure map-as="jadex.model.jibximpl.MTypedElement"></structure>
		<structure name="fact" field="defaultfact" usage="optional" type="jadex.model.jibximpl.MExpression" pre-set="preset"></structure>
	</mapping>

	<mapping class="jadex.model.jibximpl.MBeliefSet" abstract="true">
		<structure map-as="jadex.model.jibximpl.MTypedElementSet"></structure>
		<collection add-method="addDefaultFact" iter-method="iterDefaultFacts" item-type="jadex.model.jibximpl.MExpression">
			<structure name="fact" type="jadex.model.jibximpl.MExpression" pre-set="preset"></structure>
		</collection>
		<structure name="facts" field="defaultfactsexp" usage="optional" type="jadex.model.jibximpl.MExpression" pre-set="preset"></structure>
	</mapping>

	<mapping class="jadex.model.jibximpl.MParameter" abstract="true">
		<structure map-as="jadex.model.jibximpl.MTypedElement"></structure>
		<structure name="value" field="defaultvalue" usage="optional" type="jadex.model.jibximpl.MExpression" pre-set="preset"></structure>
		<structure name="bindingoptions" field="bindingoptions" usage="optional" type="jadex.model.jibximpl.MExpression" pre-set="preset"></structure>
		<value name="optional" field="optional" usage="optional" default="false" style="attribute"/>
		<value name="direction" field="direction" usage="optional" default="in" style="attribute"/>
	</mapping>

	<mapping class="jadex.model.jibximpl.MParameterSet" abstract="true">
		<structure map-as="jadex.model.jibximpl.MTypedElementSet"></structure>
		<collection add-method="addDefaultValue" iter-method="iterDefaultValues" item-type="jadex.model.jibximpl.MExpression">
			<structure name="value" type="jadex.model.jibximpl.MExpression" pre-set="preset"></structure>
		</collection>
		<structure name="values" field="defaultvaluesexp" usage="optional" type="jadex.model.jibximpl.MExpression" pre-set="preset"></structure>
		<value name="optional" field="optional" usage="optional" default="false" style="attribute"/>
		<value name="direction" field="direction" usage="optional" default="in" style="attribute"/>
	</mapping>

	<mapping class="jadex.model.jibximpl.MPlanParameter" abstract="true">
		<structure map-as="jadex.model.jibximpl.MParameter"></structure>
		<collection add-method="createInternalEventMapping" iter-method="iterInternalEventMappings" item-type="java.lang.String">
			<structure name="internaleventmapping">
				<value name="ref" style="attribute"/>
			</structure>
		</collection>
		<collection add-method="createMessageEventMapping" iter-method="iterMessageEventMappings" item-type="java.lang.String">
			<structure name="messageeventmapping">
				<value name="ref" style="attribute"/>
			</structure>
		</collection>
		<collection add-method="createGoalMapping" iter-method="iterGoalMappings" item-type="java.lang.String">
			<structure name="goalmapping">
				<value name="ref" style="attribute"/>
			</structure>
		</collection>
	</mapping>

	<mapping class="jadex.model.jibximpl.MPlanParameterSet" abstract="true">
		<structure map-as="jadex.model.jibximpl.MParameterSet"></structure>
		<collection add-method="createInternalEventMapping" iter-method="iterInternalEventMappings" item-type="java.lang.String">
			<structure name="internaleventmapping">
				<value name="ref" style="attribute"/>
			</structure>
		</collection>
		<collection add-method="createMessageEventMapping" iter-method="iterMessageEventMappings" item-type="java.lang.String">
			<structure name="messageeventmapping">
				<value name="ref" style="attribute"/>
			</structure>
		</collection>
		<collection add-method="createGoalMapping" iter-method="iterGoalMappings" item-type="java.lang.String">
			<structure name="goalmapping">
				<value name="ref" style="attribute"/>
			</structure>
		</collection>
	</mapping>

	<mapping class="jadex.model.jibximpl.MExpression" abstract="true">
<!--		<structure map-as="jadex.model.jibximpl.MReferenceableElement"></structure> Bug in jibx-->
		<structure map-as="jadex.model.jibximpl.MElement"></structure>
<!--		<value style="text" usage="optional" field="exptext"></value>-->
		<value style="text" usage="optional" set-method="setExpressionText" get-method="getExpressionText"></value>
		<collection add-method="addAssignToName" iter-method="iterAssignToNames" item-type="java.lang.String">
			<structure name="assignto">
				<value name="ref" style="attribute"/>
			</structure>
		</collection>
		<value name="exported" field="exported" usage="optional" default="false" style="attribute"/>
<!-- remove upper part when jibx bug fixed. -->
		<collection add-method="addExpressionParameter" iter-method="iterExpressionParameters" item-type="jadex.model.IMExpressionParameter">
			<structure name="parameter" type="jadex.model.jibximpl.MExpressionParameter" pre-set="preset"></structure>
		</collection>
		<value name="evaluationmode" field="evaluationmode" usage="optional" style="attribute"/>
		<value name="class" field="classname" usage="optional" style="attribute"/>
		<collection add-method="addRelevantBelief" iter-method="iterRelevantBeliefs" item-type="jadex.model.IMRelevantElement">
			<structure name="relevantbelief" type="jadex.model.jibximpl.MRelevantElement" pre-set="preset"></structure>
		</collection>
		<collection add-method="addRelevantBeliefSet" iter-method="iterRelevantBeliefSets" item-type="jadex.model.IMRelevantElement">
			<structure name="relevantbeliefset" type="jadex.model.jibximpl.MRelevantElement" pre-set="preset"></structure>
		</collection>
		<collection add-method="addRelevantGoal" iter-method="iterRelevantGoals" item-type="jadex.model.IMRelevantElement">
			<structure name="relevantgoal" type="jadex.model.jibximpl.MRelevantElement" pre-set="preset"></structure>
		</collection>
		<collection add-method="addRelevantParameter" iter-method="iterRelevantParameters" item-type="jadex.model.IMRelevantElement">
			<structure name="relevantparameter" type="jadex.model.jibximpl.MRelevantElement" pre-set="preset"></structure>
		</collection>
		<collection add-method="addRelevantParameterSet" iter-method="iterRelevantParameterSets" item-type="jadex.model.IMRelevantElement">
			<structure name="relevantparameterset" type="jadex.model.jibximpl.MRelevantElement" pre-set="preset"></structure>
		</collection>

	</mapping>

	<mapping class="jadex.model.jibximpl.MCondition" abstract="true">
		<structure map-as="jadex.model.jibximpl.MExpression"></structure>
		<value name="trigger" field="trigger" usage="optional" style="attribute"/>
	</mapping>

	<mapping abstract="true" class="jadex.model.jibximpl.MCapabilityReference">
		<structure map-as="jadex.model.jibximpl.MElement"></structure>
		<value name="file" field="file" usage="required" style="attribute"/>
	</mapping>

	<mapping abstract="true" class="jadex.model.jibximpl.MElement">
		<value name="name" field="name" usage="optional" style="attribute"/>
		<value name="description" field="description" usage="optional" style="attribute"/>
	</mapping>

	<mapping abstract="true" class="jadex.model.jibximpl.MBase">
		<structure map-as="jadex.model.jibximpl.MElement"></structure>
	</mapping>

	<mapping abstract="true" class="jadex.model.jibximpl.MEvent">
		<structure map-as="jadex.model.jibximpl.MParameterElement"></structure>
		<value name="posttoall" field="posttoall" usage="optional" style="attribute"/>
		<value name="randomselection" field="randomselection" usage="optional" style="attribute"/>
	</mapping>

	<mapping class="jadex.model.jibximpl.MInternalEvent" abstract="true">
		<structure map-as="jadex.model.jibximpl.MEvent"></structure>
	</mapping>

	<mapping class="jadex.model.jibximpl.MMessageEvent" abstract="true">
		<structure map-as="jadex.model.jibximpl.MEvent"></structure>
